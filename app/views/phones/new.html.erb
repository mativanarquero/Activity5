<%= form_for(@phone) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %></br>

	<%= f.label :brand %>
	<%= f.text_field :brand %></br>

	<%= f.label :"Year Released" %>
	<%= f.text_field :year_released %></br>

	<%= f.label :"Camera" %>
	<%= f.text_field :camera %></br>

	<%= f.label :"Mpixels" %>
	<%= f.text_field :mpixels %></br>

	<%= f.label :"Internal Storage" %>
	<%= f.text_field :internal_storage %></br>

	<%= f.label :"Number of Sim Slots" %>
	<%= f.text_field :sim_slots %></br>

	<%= f.label :"Size of Screen" %>
	<%= f.text_field :size_of_screen %></br>

	<%= f.label :"Operating System" %>
	<%= f.text_field :operating_system %></br>

	<%= f.label :"Number in Stock" %>
	<%= f.text_field :num_in_stock %></br>



	<%= f.submit %></br>
<% end %>








